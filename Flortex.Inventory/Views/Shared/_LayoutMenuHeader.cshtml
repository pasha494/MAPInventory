        <script type="text/javascript">

            var sessionExpireTimeOut = function () {
                customAlert("If you didn't perform any action in next 2 minutes then your session will be expired.", "alert");
            }

            var timeOut = function () {
                var interval = 18 * 60000;
                setTimeout(sessionExpireTimeOut, interval);
            }
            timeOut();
            $.validator.addMethod("greaterThanZero", function (value, element) {

                var x = false;
                if (value > 0)
                    x = true;

                return x;
            }, "Please select ");

            var customAlert = function (str, type, callback) {
                if (type == 'alert') {
                    smoke.alert(str, function (e) {
                        if (callback) {
                            callback();
                        }
                    }, {
                        ok: "Ok",
                        classname: "custom-class"
                    });

                }
                else if (type == 'signal') {

                    smoke.signal(str, function (e) {

                    }, {
                        duration: 3000,
                        classname: "custom-class"
                    });
                }
            }

            Mousetrap.bind(['ctrl+h', 'command+h'], function (e) {
                e.preventDefault();
                window.location.href = '@Url.Action("Index", "Home")';
            });
